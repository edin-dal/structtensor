A(i, j) := f(i) * f(j)
B(i, j, k) := f(i) * f(j) * f(k)
C(i, j, k, l) := f(i) * f(j) * f(k) * f(l)
D(i, j, k, l, s) := f(i) * f(j) * f(k) * f(l) * f(s)
E(i, j, k, l, s, t) := f(i) * f(j) * f(k) * f(l) * f(s) * f(t)
F(i, j) := g(i) * g(j)
G(i, j, k) := g(i) * g(j) * g(k)
H(i, j, k, l) := g(i) * g(j) * g(k) * g(l)
I(i, j, k, l, s) := g(i) * g(j) * g(k) * g(l) * g(s)
J(i, j, k, l, s, t) := g(i) * g(j) * g(k) * g(l) * g(s) * g(t)
K(i, j) := A(i, j) + F(i, j)
L(i, j, k) := B(i, j, k) + G(i, j, k)
M(i, j, k, l) := C(i, j, k, l) + H(i, j, k, l)
N(i, j, k, l, s) := D(i, j, k, l, s) + I(i, j, k, l, s)
O(i, j, k, l, s, t) := E(i, j, k, l, s, t) + J(i, j, k, l, s, t)
f:D(i) := (0 <= i) * (i < N)
